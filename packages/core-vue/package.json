{
  "name": "ssr-core-vue",
  "version": "5.2.3",
  "description": "core for serverless ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": false,
  "scripts": {
    "watch": "tsc -w -p ./tsconfig.cjs.json --noEmit",
    "build:types": "concurrently \" tsc -p ./tsconfig.cjs.json --emitDeclarationOnly \" \" tsc -p ./tsconfig.esm.json --emitDeclarationOnly \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/ssr.git"
  },
  "keywords": [
    "cli",
    "serverless",
    "ssr",
    "vue",
    "react"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykfe/ssr/issues"
  },
  "homepage": "https://github.com/ykfe/ssr#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "merge-stream": "^2.0.0",
    "serialize-javascript": "^3.0.0",
    "ssr-server-utils": "^5.2.3",
    "vue": "^2.6.0",
    "vue-router": "^2.0.0",
    "vue-server-renderer": "^2.6.0",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0"
  }
}
