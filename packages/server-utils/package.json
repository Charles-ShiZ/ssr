{
  "name": "ssr-server-utils",
  "version": "5.2.3",
  "description": "utils for serverless ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": false,
  "scripts": {
    "watch": "concurrently \" tsc -w -p ./tsconfig.cjs.json --emitDeclarationOnly \"  \" tsc -w -p ./tsconfig.esm.json --emitDeclarationOnly \" ",
    "build:types": "concurrently \" tsc-transpile-only -p ./tsconfig.cjs.json --emitDeclarationOnly \" \" tsc-transpile-only -p ./tsconfig.esm.json --emitDeclarationOnly \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/ssr.git"
  },
  "keywords": [
    "cli",
    "serverless",
    "ssr",
    "react",
    "vue"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykfe/ssr/issues"
  },
  "homepage": "https://github.com/ykfe/ssr#readme",
  "devDependencies": {
    "@types/js-yaml": "^3.12.3",
    "@types/shelljs": "^0.8.7",
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "http-proxy-middleware": "^1.0.6",
    "js-yaml": "^3.13.1",
    "koa2-connect": "^1.0.2",
    "path-to-regexp": "^1.7.0",
    "readable-stream": "^3.6.0",
    "shelljs": "^0.8.3",
    "ssr-types": "^5.2.3",
    "vue-template-compiler": "^2.6.12"
  }
}
