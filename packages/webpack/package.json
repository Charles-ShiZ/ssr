{
  "name": "ssr-webpack",
  "version": "5.2.3",
  "description": "webpack config for serverless ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "sideEffects": false,
  "scripts": {
    "watch": "concurrently \" tsc -w -p ./tsconfig.cjs.json --emitDeclarationOnly \"  \" tsc -w -p ./tsconfig.esm.json --emitDeclarationOnly \" ",
    "build:types": "concurrently \" tsc-transpile-only -p ./tsconfig.cjs.json --emitDeclarationOnly \" \" tsc-transpile-only -p ./tsconfig.esm.json --emitDeclarationOnly \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/ssr.git"
  },
  "keywords": [
    "webpack",
    "serverless",
    "ssr",
    "react",
    "vue"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykfe/ssr/issues"
  },
  "homepage": "https://github.com/ykfe/ssr#readme",
  "dependencies": {
    "ssr-server-utils": "^5.2.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.42.1",
    "webpack-dev-server-ssr": "^3.10.3"
  },
  "devDependencies": {
    "@types/webpack": "^4.41.8",
    "ssr-types": "^5.2.3"
  }
}
